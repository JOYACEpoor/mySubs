mode: rule
redir-port: 10811
tproxy-port: 1536
mixed-port: 10810
socks-port: 10808
port: 10809
allow-lan: true
log-level: info
ipv6: true
secret: ''
external-controller: 0.0.0.0:9090
geodata-mode: true
enable-process: true
find-process-mode: strict
geox-url:
